<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.CustomerController">
   <children>
      <GridPane prefHeight="400.0" prefWidth="600.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <ButtonBar prefHeight="40.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="6">
              <buttons>
                <Button fx:id="saveButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#saveCustomer" text="Save" />
                  <Button layoutX="235.0" layoutY="48.0" mnemonicParsing="false" onAction="#cancelCustomer" text="Cancel" />
              </buttons>
               <GridPane.margin>
                  <Insets right="50.0" />
               </GridPane.margin>
            </ButtonBar>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="101.0" prefWidth="300.0" text="ID">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="customerNameField" prefWidth="88.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <padding>
                  <Insets bottom="10.0" />
               </padding>
               <GridPane.margin>
                  <Insets left="50.0" right="50.0" />
               </GridPane.margin>
               <font>
                  <Font size="14.0" />
               </font>
            </TextField>
            <TextField fx:id="phoneNumberField" layoutX="360.0" layoutY="147.0" prefWidth="88.0" GridPane.columnIndex="1" GridPane.rowIndex="5">
               <padding>
                  <Insets bottom="10.0" />
               </padding>
               <GridPane.margin>
                  <Insets left="50.0" right="50.0" />
               </GridPane.margin>
               <font>
                  <Font size="14.0" />
               </font>
            </TextField>
            <Label alignment="CENTER" contentDisplay="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="101.0" prefWidth="300.0" text="Name" GridPane.rowIndex="1">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label alignment="CENTER" contentDisplay="CENTER" layoutX="10.0" layoutY="110.0" prefHeight="101.0" prefWidth="300.0" text="Phone Number" GridPane.rowIndex="5">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="customerAddressField" prefWidth="88.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
               <padding>
                  <Insets bottom="10.0" />
               </padding>
               <font>
                  <Font size="14.0" />
               </font>
               <GridPane.margin>
                  <Insets left="50.0" right="50.0" />
               </GridPane.margin>
            </TextField>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="101.0" prefWidth="300.0" text="Address" GridPane.rowIndex="2">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="customerIdField" alignment="CENTER" disable="true" editable="false" prefWidth="88.0" promptText="Disabled - AutoGen" GridPane.columnIndex="1">
               <padding>
                  <Insets bottom="10.0" />
               </padding>
               <font>
                  <Font size="14.0" />
               </font>
               <GridPane.margin>
                  <Insets left="50.0" right="50.0" />
               </GridPane.margin>
            </TextField>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="101.0" prefWidth="300.0" text="City" GridPane.rowIndex="3">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <ChoiceBox fx:id="customerCityChoice" prefHeight="25.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="3">
               <GridPane.margin>
                  <Insets left="50.0" />
               </GridPane.margin>
            </ChoiceBox>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="101.0" prefWidth="300.0" text="Postal Code" GridPane.rowIndex="4">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="customerPostalCodeField" prefWidth="88.0" GridPane.columnIndex="1" GridPane.rowIndex="4">
               <padding>
                  <Insets bottom="10.0" />
               </padding>
               <font>
                  <Font size="14.0" />
               </font>
               <GridPane.margin>
                  <Insets left="50.0" right="50.0" />
               </GridPane.margin>
            </TextField>
         </children>
      </GridPane>
   </children>
</AnchorPane>
